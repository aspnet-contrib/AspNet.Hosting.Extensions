{
  "version": "1.0.0-alpha2-*",

  "description": "Hosting extensions for ASP.NET Core that allow using OWIN/Katana middleware.",
  "authors": [ "Kévin Chalet" ],
  "owners": [ "Kévin Chalet" ],

  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet-contrib/AspNet.Hosting.Extensions"
  },

  "tags": [
    "aspnetcore",
    "hosting",
    "katana",
    "owin"
  ],

  "compilationOptions": {
    "nowarn": [ "CS1591" ],
    "xmlDoc": true
  },

  "dependencies": {
    "JetBrains.Annotations": { "type": "build", "version": "10.1.2-eap" },
    "Microsoft.AspNetCore.DataProtection.Abstractions": "1.0.0-*",
    "Microsoft.AspNetCore.Hosting": "1.0.0-*",
    "Microsoft.AspNetCore.Owin": "1.0.0-*",
    "Microsoft.Owin": "3.0.0"
  },

  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.ComponentModel": { "type": "build", "version": "4.0.0.0" }
      }
    }
  }
}